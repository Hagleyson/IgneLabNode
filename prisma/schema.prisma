generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Notification{
  id String @id
  recipientId String
  content String 
  category String
  readtAt DateTime? //nullable
  createdAt DateTime @default(now())

  @@index([recipientId])
}